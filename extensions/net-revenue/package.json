{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "net-revenue",
  "title": "Net Revenue",
  "description": "Show Net Revenue",
  "icon": "command-icon.png",
  "author": "alpercun",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Net Revenue",
      "subtitle": "Show Net Revenue",
      "description": "Show Net Revenue",
      "mode": "menu-bar",
      "interval": "30m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.2",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "preferences": [
    {
      "name": "webhookUrl",
      "title": "Webhook URL",
      "description": "Please enter your webhook URL here",
      "type": "textfield",
      "required": true
    },
    {
      "name": "authToken",
      "title": "Authorization Token",
      "description": "Please enter your Authorization Token here",
      "type": "password",
      "required": true
    }
  ]
}
